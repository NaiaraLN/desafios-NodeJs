<h1>Ingrese producto</h1>
<form action="/productos" method="post">
    <input id={{listProduct.title}} type="name"  name="title" placeholder="title">
    <input id={{listProduct.price}} type="number"  name="price" placeholder="price">
    <input id={{listProduct.thumbnail}} type="url"  name="thumbnail" placeholder="url">
    <button type="submit">Enviar</button>
</form>