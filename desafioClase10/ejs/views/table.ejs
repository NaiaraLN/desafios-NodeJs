<%- include('./head')-%>
<body>
    <% if(listProduct.length > 0){ %>
        <div>
            <table>
                <tr> <th>Nombre</th> <th>Precio</th> <th>Imagen</th> </tr>
                <%listProduct.forEach(function(product) {%>
                <tr>
                    <td><%=product.title%></td>
                    <td><%=product.price%></td>
                    <td><%=product.thumbnail%></td>
                </tr>
                <%});%>
            </table>
        </div>
    <% } else{ %>
        <h2>no se encontraron productos</h1>
    <% } %>
</body>

